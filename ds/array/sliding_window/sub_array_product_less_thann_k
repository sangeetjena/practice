"""
https://leetcode.com/problems/subarray-product-less-than-k/description/

"""
import math
class Solution:
    def numSubarrayProductLessThanK(self, nums: List[int], k: int) -> int:
        count=0
        i=0
        j=1
        while i<len(nums):
            if(j>len(nums) or math.prod(nums[i:j]) >= k):
                i+=1
                j=i+1
            else:
                count+=1
                j+=1
        return count

        
